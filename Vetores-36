#include <stdio.h>

int main(void) {
 int vet[10];
  int i;
  for (i=0;i<10;i++){
    printf("Valor:");
    scanf("%d",&vet[i]);
  }
  int j;
  int temp; 
  for (i=10;i>0;i--){
    for (j=0;j<=i;j++){
      if (vet[j-1]>vet[j]){
        temp = vet[j-1];
        vet[j-1] = vet[j];
        vet[j] = temp;
      }  
    }
  }
  printf("ordem vetor:");
    for (i=0;i<11;i++){
     printf("[%d]",vet[i]); 
    }
  return 0;
}
